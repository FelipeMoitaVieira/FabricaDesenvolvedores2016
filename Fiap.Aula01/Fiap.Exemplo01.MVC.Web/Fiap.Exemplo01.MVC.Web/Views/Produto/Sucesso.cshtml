@model Fiap.Exemplo01.MVC.Web.Models.Produto

@{
    ViewBag.Title = "Sucesso";
}


<h2>Sucesso</h2>
<div class="alert alert-success">
    <h3>@TempData["mensagem"]</h3>
</div>
<div>
    <p>
        Nome: @ViewBag.prod.Nome
        Valor:@ViewBag.prod.Valor
        Quantidade: @ViewBag.prod.Quantidade
    </p>
</div>
<div>
    <p>
      @Model.Nome
       @Model.Quantidade
        @Model.Valor
    </p>
</div>